# QuantumForge Enterprise Configuration
# Production Environment Variables

# ==========================================
# SECURITY CONFIGURATION
# ==========================================
SECRET_KEY=enterprise-secret-key-change-in-production
JWT_SECRET_KEY=enterprise-jwt-secret-key-change-in-production
CSRF_SECRET_KEY=enterprise-csrf-secret-key-change-in-production

# ==========================================
# DATABASE CONFIGURATION
# ==========================================
DATABASE_URL=postgresql://quantumforge_user:secure_password@db.quantumforge.internal:5432/quantumforge_prod
DATABASE_SSL_MODE=require
DATABASE_CONNECTION_POOL_SIZE=20
DATABASE_CONNECTION_MAX_OVERFLOW=30

# ==========================================
# QUANTUM HARDWARE CONFIGURATION
# ==========================================
IBMQ_TOKEN=your-enterprise-ibm-quantum-token
IBMQ_INSTANCE=your-enterprise-instance
AWS_BRAKET_REGION=us-east-1
AWS_BRAKET_ROLE_ARN=arn:aws:iam::123456789012:role/QuantumForgeBraketRole

# ==========================================
# ENTERPRISE FEATURES
# ==========================================
ENTERPRISE_MODE=true
MULTI_TENANT_ENABLED=true
AUDIT_LOGGING_ENABLED=true
COMPLIANCE_MODE=gdpr

# ==========================================
# MONITORING & ANALYTICS
# ==========================================
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
LOG_LEVEL=WARNING
METRICS_ENABLED=true
PROMETHEUS_ENABLED=true

# ==========================================
# RATE LIMITING
# ==========================================
RATE_LIMIT_STORAGE_URL=redis://redis.quantumforge.internal:6379
RATE_LIMIT_REQUESTS_PER_HOUR=10000
RATE_LIMIT_BURST_SIZE=1000

# ==========================================
# CDN & STATIC ASSETS
# ==========================================
CDN_URL=https://cdn.quantumforge.com
STATIC_URL=https://static.quantumforge.com

# ==========================================
# EMAIL CONFIGURATION
# ==========================================
SMTP_SERVER=smtp.quantumforge.com
SMTP_PORT=587
SMTP_USERNAME=noreply@quantumforge.com
SMTP_PASSWORD=secure-smtp-password
EMAIL_FROM=noreply@quantumforge.com

# ==========================================
# BACKUP CONFIGURATION
# ==========================================
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=quantumforge-backups

# ==========================================
# LOAD BALANCER CONFIGURATION
# ==========================================
LB_HEALTH_CHECK_PATH=/api/health
LB_HEALTH_CHECK_INTERVAL=30
LB_HEALTH_CHECK_TIMEOUT=10

# ==========================================
# CACHE CONFIGURATION
# ==========================================
REDIS_URL=redis://redis.quantumforge.internal:6379
CACHE_TTL_SECONDS=3600
SESSION_TTL_SECONDS=86400

# ==========================================
# ENTERPRISE INTEGRATIONS
# ==========================================
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
JIRA_API_URL=https://your-jira-instance.atlassian.net
JIRA_USERNAME=quantumforge-bot
JIRA_API_TOKEN=your-jira-api-token

# ==========================================
# COMPLIANCE & AUDIT
# ==========================================
AUDIT_LOG_RETENTION_DAYS=2555
GDPR_COMPLIANCE_ENABLED=true
SOC2_COMPLIANCE_ENABLED=true
HIPAA_COMPLIANCE_ENABLED=false

# ==========================================
# PERFORMANCE TUNING
# ==========================================
WORKER_PROCESSES=4
MAX_REQUESTS_PER_WORKER=1000
REQUEST_TIMEOUT_SECONDS=30
WORKER_TIMEOUT_SECONDS=300

# ==========================================
# FEATURE FLAGS
# ==========================================
QUANTUM_OPTIMIZATION_ENABLED=true
AI_ASSISTANT_ENABLED=true
ADVANCED_ANALYTICS_ENABLED=true
CUSTOM_INTEGRATIONS_ENABLED=true
WHITE_LABEL_ENABLED=false

# ==========================================
# SUPPORT CONFIGURATION
# ==========================================
SUPPORT_EMAIL=enterprise@quantumforge.com
SUPPORT_PHONE=1-800-QUANTUM
SUPPORT_SLACK_CHANNEL=#enterprise-support